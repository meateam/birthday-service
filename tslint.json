{
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-config-airbnb"],
  "rules": {
    "quotemark": [true, "single"],
    "no-console": false,
    "no-default-export": false,
    "no-increment-decrement": false,
    "import-name": false,
    "max-line-length": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-snake-case",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  },
  "ter-prefer-arrow-callback": false
}
